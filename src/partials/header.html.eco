<!doctype html>
<html class="no-js" lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title><%= @document.title %> | my thoughts on web dev</title>
  <%- @getBlock("meta").toHTML() %>
  <%- @getBlock("styles").toHTML() %>
  <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.0/normalize.min.css">
  <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/foundation/5.2.2/css/foundation.min.css">
  <%- @getBlock("styles").add(["/styles/z.css"]).toHTML() %>

  <%- @getBlock("scripts").add(["/bower_components/modernizr/modernizr.js"]).toHTML() %>
</head>
<body>
  <%- @getBlock("scripts").toHTML() %>
  <div class="off-canvas-wrap" data-offcanvas>
    <div class="inner-wrap">
      <nav class="tab-bar">
        <section class="left-small">
          <a class="left-off-canvas-toggle menu-icon" href="#"><span></span></a>
        </section>
        <section class="middle tab-bar-section">
          <h1 class="title"><%= @document.title %></h1>
        </section>
      </nav>

      <%- @partial('nav') %>

      <section class="main-section">
        <div class="row">
          <div class="large-12 columns">
            <br><br>
          </div>
        </div>
        <div class="row">
          <div class="large-12 columns">
